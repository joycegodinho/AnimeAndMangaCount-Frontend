 console.error(error);\r\n            }   \r\n        }\r\n        getManga();\r\n    }, []);\r\n\r\n\r\n\r\n    return (\r\n        <StyledNote>\r\n            <h3>{manga.title}</h3>\r\n            <h4>\r\n                <em>Number </em>\r\n                    <StyledMark>\r\n                        {manga.number}\r\n                    </StyledMark>\r\n            </h4>            \r\n            <ReactMarkdown source={manga.content} />\r\n        </StyledNote>\r\n    );\r\n}\r\n\r\nexport default MangaPage;"},"lineCount":null}},"error":null,"hash":"d80f31be8b205e432280317e7d1f0484","cacheData":{"env":{}}}